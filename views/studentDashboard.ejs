<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
</head>

<body>
  <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
    <a class="navbar-brand" href="#">Online Quiz</a>
    <div class="collapse navbar-collapse" id="navbarText">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <a class="nav-link" href="#">Welcome <%= user.username %> <span class="sr-only">(current)</span></a>
        </li>
        <!-- <li class="nav-item">
          <a class="nav-link" href="#">Features</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Pricing</a>
        </li> -->
      </ul>
      <span class="navbar-text">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
            <a class="nav-link" href="/users/logout"> Logout</a>
          </li>
        </ul> 
      </span>
    </div>
  </nav>
  <div class="container" role="main" style="margin-top:5rem">
      <!-- <div class="row">
        <div class="col-md-1">
          <h3>Dashboard</h3>
        </div>
      </div> -->
      <div class="row">
        <div class="col-md-6">
          <h4>List of Quizzes</h4>
        </div>
        <div class="col-md-6">
          <h4>Your Scores</h4>
        </div>
      </div>
      <div class="row">
        <div class="col-md-6">
          <ul class="list-group">
            <% quizList.forEach(q => { %>
              <li class="list-group-item">
                <a href="/users/quiz/<%=q._id%>" >
                    <%= q.quizname %>
                </a>
              </li>
            <% }); %>

           
          </ul>
        </div>
        <div class="col-md-6">
          <ul class="list-group">
             
              <%if (scoreData.length == 0) {%>
              <li class="list-group-item"> No Test Attempted</li>
              <% }
            else {
             
            
            scoreData.forEach(s => {%>
              <li class="list-group-item"> 
                Quiz Name : <%= s.quizname%> <br>
                Score : <%= s.scoreWithTotal%> <br>
                Date : <%= new Date(s.examDate).toDateString() %>

              </li>    
            <% }); 
            }
            %>
             
            
            
            
          </ul>
        </div>
      </div>
      <div class="row"></div>
  </div>
  <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>

</body>

</html>